{
  "name": "nest-computools-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "types": "./src/types/main.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env ENV_FILE=.env nest start",
    "start:dev": "cross-env ENV_FILE=.env nest start --watch",
    "start:debug": "cross-env ENV_FILE=.env nest start --debug --watch",
    "start:prod": "cross-env ENV_FILE=.env node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env ENV_FILE=.env.test jest && npm run reset-tdb",
    "test:watch": "cross-env ENV_FILE=.env.test jest --watch",
    "test:cov": "cross-env ENV_FILE=.env.test jest --coverage",
    "test:debug": "cross-env ENV_FILE=.env.test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env ENV_FILE=.env.test jest --config ./test/jest-e2e.json",
    "migration:up": "cross-env ENV_FILE=.env sequelize db:migrate",
    "migration:down": "cross-env ENV_FILE=.env sequelize db:migrate:undo:all",
    "seed:up": "cross-env ENV_FILE=.env sequelize db:seed:all",
    "seed:down": "cross-env ENV_FILE=.env sequelize db:seed:undo:all",
    "reset-tdb": "cross-env ENV_FILE=.env.test npx ts-node ./src/db/reset-tdb.ts"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.4.5",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/websockets": "^8.4.5",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "hbs": "^4.2.0",
    "nodemailer": "^6.7.5",
    "passport": "^0.5.3",
    "passport-google-oauth2": "^0.2.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.19.1",
    "sequelize-typescript": "^2.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-google-oauth2": "^0.1.5",
    "@types/sequelize": "^4.28.13",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.4.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "forceExit": true
  }
}
